{
    "name": "sparxstar-3iatlas-dictionary",
    "private": true,
    "scripts": {
        "build": "webpack",
        "watch": "webpack --watch",
        "lint": "npm run lint:js && npm run lint:css",
        "lint:js": "eslint . --fix",
        "lint:css": "stylelint \"src/**/*.css\" --fix",
        "docs:js": "jsdoc -c jsdoc.json",
        "format": "prettier --write .",
        "test": "jest",
        "prepare": "husky"
    },
    "lint-staged": {
        "*.{js,ts,jsx,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.css": [
            "stylelint --fix",
            "prettier --write"
        ],
        "*.php": [
            "phpcs -q",
            "composer run analyze:php"
        ],
        "*.{json,md,yml,yaml}": [
            "prettier --write"
        ]
    },
    "version": "0.6.0",
    "description": "SPARXSTAR 3iAtlas Dictionary - A WordPress plugin for dictionary management with SCF and WPGraphQL integration.",
    "main": "commitlint.config.js",
    "directories": {
        "doc": "docs",
        "example": "examples",
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Starisian-Technologies/sparxstar-3iatlas-dictionary.git"
    },
    "keywords": [],
    "author": "Starisian Technologies <support@starisian.com>",
    "license": "proprietary",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/Starisian-Technologies/sparxstar-3iatlas-dictionary/issues"
    },
    "homepage": "https://github.com/Starisian-Technologies/sparxstar-3iatlas-dictionary#readme",
    "devDependencies": {
        "@babel/core": "latest",
        "@babel/preset-env": "latest",
        "@babel/preset-react": "latest",
        "@eslint/js": "latest",
        "@tailwindcss/postcss": "latest",
        "@wordpress/scripts": "^31.2.0",
        "autoprefixer": "latest",
        "babel-loader": "latest",
        "clean-jsdoc-theme": "latest",
        "css-loader": "latest",
        "css-minimizer-webpack-plugin": "latest",
        "eslint": "latest",
        "eslint-config-prettier": "latest",
        "eslint-plugin-prettier": "latest",
        "eslint-plugin-react": "latest",
        "eslint-plugin-react-hooks": "latest",
        "globals": "latest",
        "husky": "latest",
        "jest": "latest",
        "jsdoc": "latest",
        "lint-staged": "latest",
        "mini-css-extract-plugin": "latest",
        "postcss": "latest",
        "postcss-loader": "latest",
        "prettier": "latest",
        "style-loader": "latest",
        "stylelint": "latest",
        "stylelint-config-standard": "latest",
        "tailwindcss": "latest",
        "terser-webpack-plugin": "latest",
        "webpack": "latest",
        "webpack-cli": "latest",
        "webpack-dev-server": "^5.2.1"
    },
    "dependencies": {
        "@apollo/client": "latest",
        "graphql": "latest",
        "lucide-react": "latest",
        "react": "latest",
        "react-dom": "latest",
        "react-virtuoso": "latest"
    },
    "overrides": {
        "webpack-dev-server": "^5.2.1"
    }
}